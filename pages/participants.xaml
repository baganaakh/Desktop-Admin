<Page x:Class="pages.participants"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:pages"
      mc:Ignorable="d" 
      d:DesignHeight="900"  d:DesignWidth="1200"
      Title="participants">

    <Grid >
        <Button Content="Edit" HorizontalAlignment="Left" Margin="997,110,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" RenderTransformOrigin="1.181,-0.633"/>
        <Button Content="Insert" HorizontalAlignment="Left" Margin="997,143,0,0" VerticalAlignment="Top" Width="75" Click="insertFunc" RenderTransformOrigin="1.181,-0.633"/>
        <Label Content="Code" HorizontalAlignment="Left" Margin="34,72,0,0" VerticalAlignment="Top"/>
        <Label Content="Улс" HorizontalAlignment="Left" Margin="27,173,0,0" VerticalAlignment="Top"/>
        <Label Content="хаяг" HorizontalAlignment="Left" Margin="518,76,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.969,0"/>
        <Label Content="утас" HorizontalAlignment="Left" Margin="511,114,0,0" VerticalAlignment="Top"/>
        <Label Content="Факс" HorizontalAlignment="Left" Margin="511,172,0,0" VerticalAlignment="Top"/>
        <Label Content="Имэйл" HorizontalAlignment="Left" Margin="752,73,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="pphone" HorizontalAlignment="Left" Height="23" Margin="600,117,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" PreviewTextInput="PreviewTextInput"/>
        <TextBox x:Name="pfax" HorizontalAlignment="Left" Height="23" Margin="600,173,0,0" TextWrapping="Wrap" Text="1234"  VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="pmail" HorizontalAlignment="Left" Height="23" Margin="853,76,0,0" TextWrapping="Wrap" Text="1234"  VerticalAlignment="Top" Width="120"/>
        <Label Content="contact" HorizontalAlignment="Left" Margin="268,71,0,0" VerticalAlignment="Top"/>
        <Label Content="төлөв" HorizontalAlignment="Left" Margin="268,114,0,0" VerticalAlignment="Top"/>
        <Label Content="modified" HorizontalAlignment="Left" Margin="268,180,0,0" VerticalAlignment="Top"/>
        <Label Content="webid" HorizontalAlignment="Left" Margin="756,116,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="pwebid" HorizontalAlignment="Left" Height="23" Margin="853,117,0,0" TextWrapping="Wrap" Text="1234"  VerticalAlignment="Top" Width="120"/>
        <Label Content="csid" HorizontalAlignment="Left" Margin="763,173,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="pcsid" HorizontalAlignment="Left" Height="23" Margin="853,173,0,0" TextWrapping="Wrap" Text="1234"  VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="pname" HorizontalAlignment="Left" Height="23" Margin="116,117,0,0" TextWrapping="Wrap" Text="1234"  VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="pcode" HorizontalAlignment="Left" Height="23" Margin="116,76,0,0" TextWrapping="Wrap" Text="1234"  VerticalAlignment="Top" Width="120"/>
        <Label Content="Нэр" HorizontalAlignment="Left" Margin="34,114,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="pcountry" HorizontalAlignment="Left" Height="23" Margin="116,173,0,0" TextWrapping="Wrap" Text="1234"  VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="paddress" HorizontalAlignment="Left" Height="23" Margin="600,76,0,0" TextWrapping="Wrap" Text="1234"  VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="pcontact" HorizontalAlignment="Left" Height="23" Margin="369,76,0,0" TextWrapping="Wrap" Text="1234"  VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="pmodify" HorizontalAlignment="Left" Height="23" Margin="369,173,0,0" TextWrapping="Wrap" Text="1234"  VerticalAlignment="Top" Width="120"/>
        <TextBlock x:Name="checkDAta" HorizontalAlignment="Left" Margin="41,316,0,541" TextWrapping="Wrap" Width="1131"><Run Text="TextBlock"/></TextBlock>
        <DataGrid x:Name="DateTable2" HorizontalAlignment="Left" Height="258" AutoGenerateColumns="False" Margin="34,379,0,263" VerticalAlignment="Center" Width="1138">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding id}" Header="ID"/>
                <DataGridTextColumn Binding="{Binding code}" Header="Code"/>
                <DataGridTextColumn Binding="{Binding name}" Header="Name"/>
                <DataGridTextColumn Binding="{Binding country}" Header="Country"/>
                <DataGridTextColumn Binding="{Binding address}" Header="Address"/>
                <DataGridTextColumn Binding="{Binding phone}" Header="Phone"/>
                <DataGridTextColumn Binding="{Binding fax}" Header="Fax"/>
                <DataGridTextColumn Binding="{Binding email}" Header="Email"/>
                <DataGridTextColumn Binding="{Binding contact}" Header="Contact"/>
                <DataGridTextColumn Binding="{Binding state}" Header="State"/>
                <DataGridTextColumn Binding="{Binding modified}" Header="Modified"/>
                <DataGridTextColumn Binding="{Binding webid}" Header="webid"/>
                <DataGridTextColumn Binding="{Binding csid}" Header="csid"/>
            </DataGrid.Columns>
        </DataGrid>
        <Button Content="New" HorizontalAlignment="Left"  VerticalAlignment="Top" Width="75" RenderTransformOrigin="1.181,-0.633" Click="newData" Margin="997,79,0,0"/>
        <Button Content="Refresh" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" RenderTransformOrigin="1.181,-0.633" Click="refreshh" Margin="997,172,0,0"/>
        <Button Content="Delete" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" RenderTransformOrigin="1.181,-0.633" Click="delete" Margin="1090,79,0,0"/>
        <Button Content="Update" HorizontalAlignment="Left" Margin="1090,110,0,0" VerticalAlignment="Top" Width="75" Click="update" RenderTransformOrigin="1.181,-0.633"/>
        <ComboBox x:Name="pstate" ItemsSource="{Binding}" SelectionChanged="sstate_SelectionChanged" DisplayMemberPath="states1" SelectedValuePath="id" HorizontalAlignment="Left" Margin="369,117,0,0" VerticalAlignment="Top" Width="120" RenderTransformOrigin="2.517,-1"/>
        <Label Content="төрөл" HorizontalAlignment="Left" Margin="15,220,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="pstate_Copy" ItemsSource="{Binding}" SelectionChanged="sstate_SelectionChanged" DisplayMemberPath="states1" SelectedValuePath="id" HorizontalAlignment="Left" Margin="116,223,0,0" VerticalAlignment="Top" Width="120" RenderTransformOrigin="2.517,-1"/>
        <Label Content="Тусгай төрөл" HorizontalAlignment="Left" Margin="15,264,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="pstate_Copy1" ItemsSource="{Binding}" SelectionChanged="sstate_SelectionChanged" DisplayMemberPath="states1" SelectedValuePath="id" HorizontalAlignment="Left" Margin="116,267,0,0" VerticalAlignment="Top" Width="120" RenderTransformOrigin="2.517,-1"/>

    </Grid>
</Page>
