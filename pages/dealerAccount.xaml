        <TextBox PreviewTextInput="PreviewTextInput" x:Name="memid" HorizontalAlignment="Left" Height="23" Margin="155,76,0,0" TextWrapping="Wrap" Text="1234"  VerticalAlignment="Top" Width="120"/>
