<Window x:Class="pages.loginScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pages"
        mc:Ignorable="d"
        Title="loginScreen" Height="450" Width="800">
    <Grid>
        <Label Content="Login" HorizontalAlignment="Left" Margin="322,76,0,0" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="281,138,0,0" Background="AliceBlue" TextWrapping="Wrap" Text="Usernamee:" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="281,168,0,0" Background="AliceBlue" TextWrapping="Wrap" Text="Password:" VerticalAlignment="Top"/>
        <Button  Content="Submit" HorizontalAlignment="Left" Margin="322,238,0,0" VerticalAlignment="Top" Width="75" Click="log_submit"/>
        <TextBox x:Name="userName" HorizontalAlignment="Left" Height="23" Margin="354,137,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="passBox" HorizontalAlignment="Left" Margin="354,168,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
